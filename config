# config for zhtlab bootloader

CONFIG_PRODUCT_NAME_TEXT	= "NEWPROJECT"

CONFIG_VERSION_TEXT		= "0.01.00.201804010"

CONFIG_VERSION_BOOT		= 0x18082600
CONFIG_VERSION_BOOT_TEXT	= "0.01.00.201808260"


CONFIG_ARCHTYPE_CORTEX_M0	= 0x01
CONFIG_ARCHTYPE_CORTEX_M0PLUS	= 0x02
CONFIG_ARCHTYPE_CORTEX_M3	= 0x30
CONFIG_ARCHTYPE_CORTEX_M4	= 0x40
CONFIG_ARCHTYPE_CORTEX_M7	= 0x70
